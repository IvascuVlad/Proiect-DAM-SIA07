<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dam_front">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DAM_front"
        android:networkSecurityConfig="@xml/network_security_config">
        <activity android:name=".MenuActivity"/>
        <activity android:name=".menu.ContractActivity"/>
        <activity android:name=".menu.PersonalActivity"/>
        <activity android:name=".menu.PositionActivity"/>
        <activity android:name=".menu.StudiesActivity"/>
        <activity android:name=".menu.UserActivity"/>
        <activity android:name=".contract.ContractGetActivity"/>
        <activity android:name=".studies.StudiesGetActivity"/>
        <activity android:name=".studies.StudiesCreateActivity"/>
        <activity android:name=".studies.StudiesUpdateActivity"/>
        <activity android:name=".studies.StudiesDeleteActivity"/>
        <activity android:name=".personal.PersonalCreateActivity"/>
        <activity android:name=".personal.PersonalGetActivity"/>
        <activity android:name=".personal.PersonalUpdateActivity"/>
        <activity android:name=".personal.PersonalDeleteActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>